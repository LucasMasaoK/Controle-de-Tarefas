    editSenha: TLabeledEdit;
    editConfirmSenha: TLabeledEdit;
    comboDireito: TComboBox;
    Label1: TLabel;
  private
    procedure verificaSenha;
  public
    { Public declarations }
  end;

var
  frmCadCliente: TfrmCadCliente;

