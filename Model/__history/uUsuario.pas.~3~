unit uUsuario;

interface

type
  TUsuario = class
  private
    FCodigo: integer;
    FNome: string;
    FDireito: string;
    FSenha: string;

  public
    property Codigo: integer read FCodigo write FCodigo;
    property Nome: string read FNome write FNome;
    property Direito: string read FDireito write FDireito;
    property Senha: string read FSenha write FSenha;
  end;

implementation

end.
